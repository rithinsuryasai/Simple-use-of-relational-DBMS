SET FOREIGN_KEY_CHECKS=0;
ALTER TABLE employee add CONSTRAINT FK_SUPERSSN FOREIGN KEY(Super_ssn) references employee(Ssn) on delete set null on update cascade;
SET FOREIGN_KEY_CHECKS=1;
ALTER TABLE EMPLOYEE ADD CONSTRAINT FK_DNO FOREIGN KEY (Dno) REFERENCES DEPARTMENT(Dnumber) ON DELETE NO ACTION ON UPDATE cascade;
ALTER TABLE DEPARTMENT ADD CONSTRAINT FK_MGRSSNN FOREIGN KEY (Mgr_ssn) REFERENCES EMPLOYEE(Ssn)  on delete RESTRICT ON UPDATE cascade;
ALTER TABLE DEPT_LOCATIONS ADD CONSTRAINT FK_DNOLOC FOREIGN KEY (Dnumber) REFERENCES DEPARTMENT(Dnumber) ON DELETE RESTRICT ON UPDATE cascade;
ALTER TABLE PROJECT ADD CONSTRAINT FK_DNUM FOREIGN KEY (Dnum) REFERENCES DEPARTMENT(Dnumber) ON DELETE CASCADE ON UPDATE cascade;
ALTER TABLE WORKS_ON ADD CONSTRAINT FK_WN FOREIGN KEY(Essn) REFERENCES EMPLOYEE(Ssn) ON DELETE CASCADE ON UPDATE cascade;
SET FOREIGN_KEY_CHECKS=0;
ALTER TABLE WORKS_ON ADD CONSTRAINT FK_PNO FOREIGN KEY(Pno) REFERENCES PROJECT(Pnumber) ON DELETE CASCADE ON UPDATE cascade;
SET FOREIGN_KEY_CHECKS=1;
ALTER TABLE DEPENDENT ADD CONSTRAINT FK_ESSN FOREIGN KEY(Essn) REFERENCES EMPLOYEE(Ssn);